na be a problem anymore. ","#","^speaker:Mercury","/#","\n","^Good. ","#","^speaker:Moon","/#","\n","^... ","#","^speaker:Moon","/#","\n","^No promises on my side though lol ","#","^speaker:Moon","/#","\n","^... ","#","^speaker:Mercury","/#","\n","^HUH?? ","#","^speaker:Mercury","/#","\n","end",{"#f":1}],"#f":1}],"global decl":["ev",1,{"VAR=":"currentDay"},0,{"VAR=":"dayInteractions"},0,{"VAR=":"interactions"},0,{"VAR=":"affinity"},false,{"VAR=":"smell"},false,{"VAR=":"ex"},"/ev","end",null],"#f":1}],"listDefs":{}}